<section class="main">
  <div class="content">
    <% page.posts.each(function (post) { %>
      <a class="post-item" href="<%- url_for(post.path) %>">
        <span class="post-date"><%- date(post.date) %></span>
        <h3 class="post-title"><%- post.title %></h3>
        <!--<span class="post-monment">( 123 )</span>-->
      </a>
    <% }) %>
  </div>
  <div class="sidebar"></div>
</section>